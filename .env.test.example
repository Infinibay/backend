# Example .env.test for running DB-backed tests locally
# Adjust credentials for your local Postgres
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/infinibay_test?schema=public"

# Optional settings used by the app
FRONTEND_URL="*"
TOKENKEY=TEST_TOKEN
PORT=4001
RPC_URL="http://localhost:9090"

# Infinibay specific
INFINIBAY_BASE_DIR=/opt/infinibay
INFINIBAY_STORAGE_POOL_NAME=infinibay
GRAPHIC_HOST=127.0.0.1
BRIDGE_NAME=br0

